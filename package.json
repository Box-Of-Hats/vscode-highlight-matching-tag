{
    "name": "higlight-matching-tag",
    "displayName": "higlight-matching-tag",
    "description": "Highlights matching closing or opening tag",
    "version": "0.0.1",
    "publisher": "vincaslt",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "highlight-matching-tag.highlight",
            "title": "Highlight matching opening or closing tag"
        }],
        "configuration": {
            "highlight-matching-tag.enabled": {
                "type": "boolean",
                "default": "true",
                "description": "Enable or disable the highlighting."
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}